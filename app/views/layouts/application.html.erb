<!DOCTYPE html>
<%= render 'layouts/inspector_header' if Rails.env == "production" %>
<html lang='<%= I18n.locale %>'>
<head>
  <%= render 'layouts/head', layout: 'application' %>
</head>
<body>
  <%= render 'layouts/navbar' %>
  <%= render 'cookies_eu/cookies_eu', link: page_path(id: 'cookies', locale: I18n.locale) %>
  <div class="container">
    <div class="row">
      <div class="col-sm-12">
        <%= yield %>
      </div>
    </div>
  </div><!-- /.container -->

  <%= render 'layouts/messages' %>
  
  <%= render 'layouts/footer' %>

  <script type="text/javascript">
    moment.locale('<%= I18n.locale %>');
  </script>
  <%= yield :scripts %>

</body>
</html>
