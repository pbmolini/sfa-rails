<% title _("Bookings for %{boat_name}") %{boat_name: @boat.name} %>
<div class="row">
  <div class="col-sm-10 col-sm-offset-1">
    <h1><%= _("Received Messages") %></h1>
    <%= render 'bookings_list', bookings: @bookings %>
  </div>
  <div class="col-sm-10 col-sm-offset-1">
    <div class="list-group">
      <br>
      <%= link_to("Back to #{@boat.name}", boat_path(@boat)) if @boat %>
    </div>
  </div>
</div>